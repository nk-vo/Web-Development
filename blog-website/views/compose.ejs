<%- include ('partials/header') -%>

  <h1>Compose</h1>


  <form action="/compose" method="post">
    <div class="form-group">
      <label for="post-title">Title</label>
      <input type="text" name="postTitle" class="form-control" id="post-title">
      <label for="post-body">Post</label>
      <textarea name="postBody" id="post-body" cols="30" rows="5" class="form-control"></textarea>
    </div>

    <button type="submit" name="button" class="btn btn-primary" >Publish</button>
  </form>

<%- include ('partials/footer') -%>